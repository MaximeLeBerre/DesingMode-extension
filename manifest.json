{
  "name": "Design Mode",
  "description": "Editing website never be so easy",
  "version": "0.0.0.1",
  "manifest_version": 3,
  "icons": {
    "16": "logo/16.png",
    "32": "logo/32.png",
    "48": "logo/48.png",
    "128": "logo/128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  }
}